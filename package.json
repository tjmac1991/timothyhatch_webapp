{
  "name": "timothyhatch-webapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && npm run copy && vite build",
    "serve": "vite preview",

    "eject": "react-scripts eject",

    "copy": "npm run copy-cmaps && npm run copy-standard-fonts && npm run copy-worker",
    "copy-cmaps": "node --loader ts-node/esm ./scripts/copy-cmaps.ts",
    "copy-standard-fonts": "node --loader ts-node/esm ./scripts/copy-standard-fonts.ts",
    "copy-worker": "node --loader ts-node/esm ./scripts/copy-worker.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^7.7.1",
    "react-router-dom": "^6.22.3",
    "sass": "^1.71.1",
    "typescript": "^4.9.5",
    "vite": "^5.1.6",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "ts-node": "^10.9.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
